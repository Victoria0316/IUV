public class BannerImageResponse extends IuwHttpResponse {


    public ArrayList<BannerImageInfo> data = new ArrayList<BannerImageInfo>();

    public static class BannerImageInfo {
        public String id;//主键;
        public String attrDeptId;//归属组织ID;
        public String attrDeptCode;// 归属组织代号;
        public String attrDeptName;// 归属组织名称;
        public String attrType;//图片类型（1、商场2、商品3、宝藏）;
        public String imagePath;//图片路径;
    }

}
