package bluemobi.iuv.network.response;

import bluemobi.iuv.network.IuwHttpResponse;

/**
 * Created by liufy on 2015/10/10.
 */
public class StoreDetailsResponse extends IuwHttpResponse {

    private StoreDetailsData data;

    public StoreDetailsData getData() {
        return data;
    }

    public void setData(StoreDetailsData data) {
        this.data = data;
    }

    public static class StoreDetailsData {
        public String id;//主键
        public String shopsTypeId;//商场类型ID
        public String shopsTypeName;//商场类型名称
        public String shopNo;//商场编号
        public String shopName;//商场名称
        public String shopsBusinessHours;//营业时间
        public String shopsWebsiteAddress;//官网地址
        public String shopsPromotionInfo;//促销信息
        public String shopIntroduce;//店铺介绍
        public String collectionNum;//收藏数
        public String praiseNum;//点赞数
        public String commentNum;//点评数
        public String shopCellphone;//商场电话
        public String shopAddress;//地址
        public String shopsLongitude;//经度
        public String latitude;//纬度
        public String shopImgPath;//图片路径
        public String praiseId;//点赞ID,未点赞为空;
        public String collectionId;//收藏ID,未收藏为空;

        public String shopCityCode;//:所在城市代码
        public String shopCityName;//:所在城市名称
        public String shopDistrictCode;//:所在区代码;
        public String shopDistrictName;//:所在区名称;
        public String preciousId;//:宝藏ID
    }


}
